    "version": "0.2.0",
    "configurations": [
        {
            "name": "sphinx-autobuild",
            "type": "debugpy",
            "request": "launch",
            "module": "sphinx_autobuild",
            "args": [
                "source",
                "build/html",
                "--ignore",
                "source/_tags",
                "-j",
                "auto"
            ]
        },
        {
            "name": "sphinx build html",
            "type": "debugpy",
            "request": "launch",
            "module": "sphinx.cmd.build",
            "args": [
                "-M",
                "html",
                "source",
                "build/html"
            ]
        }
    ]
}